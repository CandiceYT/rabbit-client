<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".TracerTestActivity">

    <com.susion.rabbit.ui.base.view.RabbitActionBar
        android:id="@+id/mTracerPageActionBar"
        android:layout_width="match_parent"
        android:layout_height="45dp" />

    <TextView
        android:id="@+id/mTracerPageTvSimpleList"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:gravity="left|center_vertical"
        android:text="简单列表"
        android:textColor="@color/rabbit_black"
        android:textSize="18dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginStart="15dp"
        android:background="@color/devtools_divider_graye5" />


    <TextView
        android:id="@+id/mTracerPageTvBlock2S"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:gravity="left|center_vertical"
        android:text="卡顿2秒"
        android:textColor="@color/rabbit_black"
        android:textSize="18dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginStart="15dp"
        android:background="@color/devtools_divider_graye5" />

</LinearLayout>